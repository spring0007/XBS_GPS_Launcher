package com.sczn.wearlauncher.socket.command.obtain;

/**
 * Description:
 * Created by Bingo on 2019/2/27.
 */
public class IntimateSettingObtain {

    /**
     * bootTime : null
     * isAutoAnswer : false
     * isCallPosition : false
     * isPowerSaving : false
     * isReservePower : true
     * isShieldStrangeCall : false
     * shutdownTime : null
     * isNoShutdown : false
     * isEnableVolte : false
     * isAutoShutdown : false
     * isAutoBoot : false
     * watchId : 3
     */

    private String bootTime;        //开机时间
    private boolean isAutoAnswer;   //自动接听
    private boolean isCallPosition; //通话位置
    private boolean isPowerSaving;  //省电模式
    private boolean isReservePower; //预留电量
    private boolean isShieldStrangeCall;   //屏蔽陌生呼叫
    private String shutdownTime;   //关机时间
    private boolean isNoShutdown;  //禁止关机
    private boolean isEnableVolte; //启用volte高清通话
    private boolean isAutoShutdown;//定时关机
    private boolean isAutoBoot;    //定时开机
    private int watchId;

    public String getBootTime() {
        return bootTime;
    }

    public void setBootTime(String bootTime) {
        this.bootTime = bootTime;
    }

    public boolean getIsAutoAnswer() {
        return isAutoAnswer;
    }

    public void setIsAutoAnswer(boolean isAutoAnswer) {
        this.isAutoAnswer = isAutoAnswer;
    }

    public boolean getIsCallPosition() {
        return isCallPosition;
    }

    public void setIsCallPosition(boolean isCallPosition) {
        this.isCallPosition = isCallPosition;
    }

    public boolean getIsPowerSaving() {
        return isPowerSaving;
    }

    public void setIsPowerSaving(boolean isPowerSaving) {
        this.isPowerSaving = isPowerSaving;
    }

    public boolean getIsReservePower() {
        return isReservePower;
    }

    public void setIsReservePower(boolean isReservePower) {
        this.isReservePower = isReservePower;
    }

    public boolean getIsShieldStrangeCall() {
        return isShieldStrangeCall;
    }

    public void setIsShieldStrangeCall(boolean isShieldStrangeCall) {
        this.isShieldStrangeCall = isShieldStrangeCall;
    }

    public String getShutdownTime() {
        return shutdownTime;
    }

    public void setShutdownTime(String shutdownTime) {
        this.shutdownTime = shutdownTime;
    }

    public boolean getIsNoShutdown() {
        return isNoShutdown;
    }

    public void setIsNoShutdown(boolean isNoShutdown) {
        this.isNoShutdown = isNoShutdown;
    }

    public boolean getIsEnableVolte() {
        return isEnableVolte;
    }

    public void setIsEnableVolte(boolean isEnableVolte) {
        this.isEnableVolte = isEnableVolte;
    }

    public boolean getIsAutoShutdown() {
        return isAutoShutdown;
    }

    public void setIsAutoShutdown(boolean isAutoShutdown) {
        this.isAutoShutdown = isAutoShutdown;
    }

    public boolean getIsAutoBoot() {
        return isAutoBoot;
    }

    public void setIsAutoBoot(boolean isAutoBoot) {
        this.isAutoBoot = isAutoBoot;
    }

    public int getWatchId() {
        return watchId;
    }

    public void setWatchId(int watchId) {
        this.watchId = watchId;
    }
}
