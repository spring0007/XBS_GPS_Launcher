package cn.com.waterworld.alarmclocklib;

import android.content.Context;
import android.support.v7.app.AppCompatActivity;

import cn.com.waterworld.alarmclocklib.anzewei.parallaxbacklayout.ParallaxBack;
import cn.com.waterworld.alarmclocklib.anzewei.parallaxbacklayout.widget.ParallaxBackLayout;


@ParallaxBack(edge = ParallaxBack.Edge.LEFT, layout = ParallaxBack.Layout.COVER)
public class BaseActivity extends AppCompatActivity implements ParallaxBackLayout.ICanDrawBackgroundView {

    @Override
    protected void attachBaseContext(Context newBase) {
        super.attachBaseContext(newBase);


    }

    @Override
    public boolean canDrawBackgroundView() {
        return true;
    }
}
