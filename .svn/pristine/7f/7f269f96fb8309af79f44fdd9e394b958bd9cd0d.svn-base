package com.sczn.wearlauncher.setting.bean;

import android.net.NetworkInfo;
import android.net.wifi.WifiInfo;



public class WifiAssistantBean {
    private WifiInfo wifiInfo;
    private int wifiState;
    private NetworkInfo.State wifiNetworkState;

    public WifiInfo getWifiInfo() {
        return wifiInfo;
    }

    public void setWifiInfo(WifiInfo wifiInfo) {
        this.wifiInfo = wifiInfo;
    }

    public int getWifiState() {
        return wifiState;
    }

    public void setWifiState(int wifiState) {
        this.wifiState = wifiState;
    }

    public NetworkInfo.State getWifiNetworkState() {
        return wifiNetworkState;
    }

    public void setWifiNetworkState(NetworkInfo.State wifiNetworkState) {
        this.wifiNetworkState = wifiNetworkState;
    }
}
