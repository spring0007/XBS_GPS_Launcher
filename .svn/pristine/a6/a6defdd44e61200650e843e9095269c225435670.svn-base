<resources>

    <declare-styleable name="LetterView">
        <attr name="letters" format="reference" />
        <attr name="showcount" format="integer" />
        <attr name="textsize_normal" format="dimension" />
        <attr name="textsize_selected" format="dimension" />
        <attr name="color_normal" format="color" />
        <attr name="color_selected" format="color" />
        <attr name="need_loop" format="boolean" />
    </declare-styleable>

    <declare-styleable name="Menu">
        <attr name="CustomName" format="string" />
        <attr name="CustomIconName" format="string" />
        <attr name="ClassName" format="string" />
        <attr name="Type" format="integer" />
        <attr name="Action" format="string" />
    </declare-styleable>

    <declare-styleable name="MenuMore">
        <attr name="CropToCircle" format="boolean" />
        <attr name="BorderColor" format="dimension" />
        <attr name="BorderWidth" format="reference" />
    </declare-styleable>

    <!--  -->
    <declare-styleable name="LanTinTextView">
        <attr name="style" format="string" />
    </declare-styleable>

    <declare-styleable name="CircleBarView">
        <attr name="progress_color" format="color" />
        <attr name="progress_colorBg" format="color" />
        <attr name="padingOut" format="dimension" />
        <attr name="stroke_width" format="dimension" />
    </declare-styleable>

    <declare-styleable name="MultiViewPager">
        <!-- Child view inside a page to match the page size against. -->
        <attr name="matchChildWidth" format="reference" />
        <!-- Maximum size. -->
        <attr name="android:maxWidth" />
        <!-- Maximum size. -->
        <attr name="android:maxHeight" />
    </declare-styleable>

    <declare-styleable name="MenuItem">
        <attr name="is_app_item" format="boolean" />
        <attr name="item_translation_max" format="reference" />
    </declare-styleable>

    <!-- card activity -->
    <declare-styleable name="ChartView">
        <attr name="maxValue" format="integer" />
        <attr name="dividerCount" format="integer" />
        <attr name="lineColor" format="color" />
        <attr name="textColor" format="color" />
        <attr name="pathStartColor" format="color" />
        <attr name="pathMidColor" format="color" />
        <attr name="pathEndColor" format="color" />

        <attr name="pathEffect" format="integer" />
        <attr name="xString" format="reference" />
        <attr name="yString" format="reference" />
    </declare-styleable>

    <!-- status -->
    <declare-styleable name="StatusIcon">
        <attr name="iconsize" format="dimension" />
        <attr name="textpadding" format="dimension" />
        <attr name="textsize" format="dimension" />
        <attr name="stringId" format="reference" />
        <attr name="imageId" format="reference" />
    </declare-styleable>

    <!-- bg -->
    <declare-styleable name="BgStyle">
        <attr name="forWhich">
            <enum name="menu" value="0" />
            <enum name="status" value="1" />
            <enum name="notification" value="2" />
            <enum name="shortcut" value="3" />
            <enum name="card" value="4" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>


    <!-- 交朋友 -->
    <declare-styleable name="drawableSwitch">
        <attr name="radius" format="dimension" />
        <attr name="switchOnColor" format="color" />
        <attr name="switchOffColor" format="color" />
        <attr name="circleColor" format="color" />
        <attr name="switchTextColor" format="color" />
        <attr name="isSwitchOn" format="boolean" />
    </declare-styleable>

</resources>
