<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:dither="true"
            android:shape="rectangle">

            <corners android:radius="2dp" />
            <stroke
                android:width="2dp"
                android:color="@color/gray" />

        </shape>
    </item>

    <item
        android:bottom="2dp"
        android:top="0dp">
        <shape
            android:dither="true"
            android:shape="rectangle">
            <corners android:radius="2dp" />
            <solid android:color="@color/gray" />
        </shape>
    </item>


</layer-list>